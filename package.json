    "@vitest/ui": "^4.0.14",
    "autoprefixer": "^10.4.22",
    "electron": "^38.1.2",
    "electron-builder": "^25.1.8",
    "electron-vite": "^4.0.1",
    "eslint": "^9.36.0",
    "eslint-plugin-solid": "^0.14.5",
    "happy-dom": "^20.0.11",
    "postcss": "^8.5.6",
